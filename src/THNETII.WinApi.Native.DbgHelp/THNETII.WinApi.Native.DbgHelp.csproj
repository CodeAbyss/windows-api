<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>netstandard1.3</TargetFramework>
    <NetStandardImplicitPackageVersion>1.6.1</NetStandardImplicitPackageVersion>
    <PackageTargetFallback>$(PackageTargetFallback);dotnet5.4;portable-net452+win81</PackageTargetFallback>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <DebugType>portable</DebugType>

    <RootNamespace>Microsoft.Win32.WinApi.Diagnostics.DbgHelp</RootNamespace>

    <VersionSufix>msdnlibrary-ms679309</VersionSufix>
    <Description></Description>
  </PropertyGroup>

  <Import Project="..\..\common.props" />
  <Import Project="..\..\version.props" />

  <ItemGroup>
    <ProjectReference Include="..\..\thnetii-interopservices\src\THNETII.InteropServices.Bitfields\THNETII.InteropServices.Bitfields.csproj" />
    <ProjectReference Include="..\..\thnetii-interopservices\src\THNETII.InteropServices.NativeMemory\THNETII.InteropServices.NativeMemory.csproj" />
    <ProjectReference Include="..\THNETII.WinApi.Native.WinError\THNETII.WinApi.Native.WinError.csproj" />
  </ItemGroup>

  <ItemGroup>
    <PackageReference Include="System.IO.MemoryMappedFiles" Version="4.3.0" />
  </ItemGroup>

  <ItemGroup>
    <Compile Remove="obj\**\*.cs" />
    <Compile Remove="bin\**\*.cs" />
    <None Remove="obj\**\*.*" />
    <None Remove="bin\**\*.*" />
  </ItemGroup>

</Project>
