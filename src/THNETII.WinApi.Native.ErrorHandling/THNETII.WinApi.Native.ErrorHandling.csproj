<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Library</OutputType>
    <TargetFramework>netstandard1.1</TargetFramework>
    <NetStandardImplicitPackageVersion>1.6.1</NetStandardImplicitPackageVersion>
    <PackageTargetFallback>$(PackageTargetFallback);dotnet5.2;portable-net45+win8</PackageTargetFallback>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
    <DebugType>portable</DebugType>

    <RootNamespace>Microsoft.Win32.WinApi.Diagnostics.ErrorHandling</RootNamespace>

    <VersionSufix>msdnlibrary-ms679320</VersionSufix>
    <Description></Description>
  </PropertyGroup>

  <Import Project="..\..\common.props" />
  <Import Project="..\..\version.props" />

  <ItemGroup>
    <Compile Remove="obj\**\*.cs" />
    <Compile Remove="bin\**\*.cs" />
    <None Remove="obj\**\*.*" />
    <None Remove="bin\**\*.*" />
  </ItemGroup>

</Project>
